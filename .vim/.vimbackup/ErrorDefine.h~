// LUX (IBEO-Laser)
#define E_DETECT_LUX                        0xE50

#define E_LUX_NORESPONCE1                   0xE51    // LBI-LUXの通信異常
#define E_LBI_HEARTBEAT1                    0xE52    // SAFETY-LBIの通信異常
#define E_LBI_NOT_READY1                    0xE53    // LBIが起動していない
#define E_LUX_MOTOR1                        0xE54    // LUX自己診断
#define E_LUX_IF_ERROR1                     0xE55    // LUX自己診断
#define E_LUX_CONFIG1                       0xE56    // LUX自己診断
#define E_LUX_TIMEOUT1                      0xE57    // LUX自己診断
#define E_LUX_OTHER1                        0xE58    // LUX自己診断
#define E_LBI_NPOSITION1                    0xE59    // LBIがNPOSITIONを受信していない
#define E_LBI_NO_CLIPAREA1                  0xE5A    // LBIのクリッピングエリアが空
#define E_LBI_PARAMETER1                    0xE5B    // LBIのパラメータ異常
#define E_LUX_WRONG_MODE1                   0xE5C    // LUXの動作モード異常
#define E_LUX_PACKET_LOST1                  0xE5D    // LBIからTargetPacketがこない
#define E_LUX_BLINDNESS1                    0xE5E

#define E_LUX_NORESPONCE2                   0xE61
#define E_LBI_HEARTBEAT2                    0xE62
#define E_LBI_NOT_READY2                    0xE63
#define E_LUX_MOTOR2                        0xE64
#define E_LUX_IF_ERROR2                     0xE65
#define E_LUX_CONFIG2                       0xE66
#define E_LUX_TIMEOUT2                      0xE67
#define E_LUX_OTHER2                        0xE68
#define E_LBI_NPOSITION2                    0xE69
#define E_LBI_NO_CLIPAREA2                  0xE6A
#define E_LBI_PARAMETER2                    0xE6B
#define E_LUX_WRONG_MODE2                   0xE6C    // LUXの動作モード異常
#define E_LUX_PACKET_LOST2                  0xE6D    // LBIからTargetPacketがこない
#define E_LUX_BLINDNESS2                    0xE6E


