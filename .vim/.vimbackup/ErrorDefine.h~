#ifndef _ERROR_DEFINE_H
#define _ERROR_DEFINE_H

/* LH Safety */
#define E_CANT_STOP_ENGINE                  0xC01
#define E_CANT_START_ENGINE                 0xC02
#define E_NO_FLIGHT_RECORDER                0xC03
#define E_NO_CF_CARD                        0xC04
#define E_WRONG_FR_POWER                    0xC05
#define E_NVRAM_CHECKSUM                    0xC06
#define E_TOO_MANY_OBSTACLE                 0xC07
#define E_ENGINE_STOPPED                    0xC08
#define E_SF_STOPPED_ENGINE                 0xC09
#define E_NVRAM_CLEARED                     0xC0A
#define E_EG_STOPPED_CAUTION                0xC0B
#define E_NVRAM_NEED_ODSCALIB               0xC0C
#define E_FR_NO_CANCOMM                     0xC0D
#define E_FR_NO_LAN		                    0xEF0
#define E_FR_CF_ERR		                    0xEF1


/* 930Eから追加 */
#define E_MA_KEYSHORT                       0xC11
#define E_MA_KEYCUT                         0xC12

/* 電源電圧関係 */
#define E_LOWBAT_BATT                       0xC13
#define E_LOWBAT_KEY                        0xC14
#define E_LOWBAT_VIS                        0xC15

#define E_POWER_RELAY_NO_WORK               0xC19
#define E_POWER_RELAY_FAILURE               0xC1A

#define E_COURSE_OUT_WRONG_POSITION         0xC1B
#define E_TRUCK_KEEP_MOVING                 0xC1C
#define E_TRUCK_WRONG_DIRECTION             0xC1D
#define E_TRUCK_NOT_REACH_TARGET            0xC1E
#define E_DRIVE_DISCARD_E_STOP              0xC1F


/* Touch sensor */
#define E_TOUCH_SENSOR                      0xC20

/* ODS software */
#define E_ZONE_BUFFER_EMPTY                 0xC30
#define E_RBI_CANID                         0xC31
#define E_ACC3_NOT_READY                    0xC32

/* Output Check */
#define E_R_LAMP_OPEN                       0xC40
#define E_G_LAMP_OPEN                       0xC41
#define E_Y_LAMP_OPEN                       0xC42
#define E_EG_START_OPEN                     0xC43
#define E_RADAR_POWER_OPEN                  0xC44
#define E_HEADLIGHT_OPEN                    0xC45
#define E_HEADLIGHT_HI_OPEN                 0xC46//U48
#define E_HORN_OPEN                         0xC47
#define E_RESTREQ_OPEN                      0xC49
#define E_PARKING_BRAKE_OPEN                0xC4A//U48
#define E_AUTOAPPLY_OPEN                    0xC4B
#define E_TURN_L_LIGHT_OPEN                 0xC4C
#define E_TURN_R_LIGHT_OPEN                 0xC4D
#define E_BRAKE_LOCK_OPEN                   0xC4E
#define E_EG_STOP_OPEN                      0xC4F

#define E_R_LAMP_GNDSHORT                   0xC50
#define E_G_LAMP_GNDSHORT                   0xC51
#define E_Y_LAMP_GNDSHORT                   0xC52
#define E_EG_START_GNDSHORT                 0xC53
#define E_RADAR_POWER_GNDSHORT              0xC54
#define E_HEADLIGHT_GNDSHORT                0xC55
#define E_HEADLIGHT_HI_GNDSHORT             0xC56//U48
#define E_HORN_GNDSHORT                     0xC57
#define E_RESTREQ_GNDSHORT                  0xC59
#define E_PARKING_BRAKE_GNDSHORT            0xC5A//U48
#define E_AUTOAPPLY_GNDSHORT                0xC5B
#define E_TURN_L_LIGHT_GNDSHORT             0xC5C
#define E_TURN_R_LIGHT_GNDSHORT             0xC5D
#define E_BRAKE_LOCK_GNDSHORT               0xC5E
#define E_EG_STOP_GNDSHORT                  0xC5F

#define E_R_LAMP_HOTSHORT                   0xC60
#define E_G_LAMP_HOTSHORT                   0xC61
#define E_Y_LAMP_HOTSHORT                   0xC62
#define E_EG_START_HOTSHORT                 0xC63
#define E_RADAR_POWER_HOTSHORT              0xC64
#define E_HEADLIGHT_HOTSHORT                0xC65
#define E_HEADLIGHT_HI_HOTSHORT             0xC66//U48
#define E_HORN_HOTSHORT                     0xC67
#define E_RESTREQ_HOTSHORT                  0xC69
#define E_PARKING_BRAKE_HOTSHORT            0xC6A//U48
#define E_AUTOAPPLY_HOTSHORT                0xC6B
#define E_TURN_L_LIGHT_HOTSHORT             0xC6C
#define E_TURN_R_LIGHT_HOTSHORT             0xC6D
#define E_BRAKE_LOCK_HOTSHORT               0xC6E
#define E_EG_STOP_HOTSHORT                  0xC6F

/* Software Exception */
#define E_SOFTWARE_EXCEPTION1               0xC70
#define E_SOFTWARE_EXCEPTION2               0xC73
#define E_SOFTWARE_EXCEPTION3               0xC77


/* 車両コーションパネル(930E) */
#define E_LOW_STEERING_PRESSURE             0xC90
#define E_LOW_ACCUMULATOR_PRESS             0xC91
#define E_LOW_BRAKE_PRESSURE                0xC92
#define E_LOW_OIL_TANK_LEVEL                0xC93
#define E_STOP_ENGINE_E3                    0xC94
#define E_NO_PROPEL_RETARD                  0xC95
#define E_OIL_TEMP_HIGH                     0xC96
#define E_NO_PROPEL                         0xC98
#define E_BRAKE_OIL_TMP_HIGH                0xC99
#define E_PROPEL_SYS_AT_REST                0xC9A
#define E_PROPEL_NOT_READY                  0xC9C
#define E_IFM_WARNING                       0xC9D
#define E_BATTERY_CHARGE                    0xC9E


/* 以下はコーション */
#define E_LOW_AUTO_LUBE                     0xCA0
#define E_CIRCUIT_BREAK                     0xCA1
#define E_OIL_FILTER                        0xCA2
#define E_LOW_FUEL                          0xCA3
#define E_5MIN_SHUTDOWN_TIMER               0xCA4
#define E_CHECK_ENGINE                      0xCA5
#define E_PROPEL_SYS_CAUTION                0xCA6
#define E_PROPEL_TEMP_CAUTION               0xCA7
#define E_REDUCED_PROPEL                    0xCAA
#define E_RETARD_AT_REDUCE                  0xCAB
#define E_ENGINE_SIG_CAUTION                0xCAC
#define E_EMERGENCY_STEERING                0xCAD

#define E_LADDER_WRONG_OPTION               0xCB0
#define E_LADDER_WIRING                     0xCB1

/* HEMicrowave (ACC3) */
#define E_DETECT_RADAR1                     0xD00
#define E_DETECT_RADAR2                     0xD30
#define E_DETECT_RADAR3                     0xD60
#define E_DETECT_RADAR4                     0xD90

#define E_RBI_HEARTBEAT0                    0xD01     // Safety-RBIの通信異常
#define E_RBI_HEARTBEAT1                    0xD31
#define E_RBI_HEARTBEAT2                    0xD61
#define E_RBI_HEARTBEAT3                    0xD91

#define E_ACC3_SCANINDEX0                   0xD02
#define E_ACC3_SCANINDEX1                   0xD32
#define E_ACC3_SCANINDEX2                   0xD62
#define E_ACC3_SCANINDEX3                   0xD92

#define E_ACC3_NORESPONCE0                  0xD03      // RBI-ACC3の通信異常
#define E_ACC3_NORESPONCE1                  0xD33
#define E_ACC3_NORESPONCE2                  0xD63
#define E_ACC3_NORESPONCE3                  0xD93

#define E_ACC3_LOWSENSITIVE0                0xD04
#define E_ACC3_LOWSENSITIVE1                0xD34
#define E_ACC3_LOWSENSITIVE2                0xD64
#define E_ACC3_LOWSENSITIVE3                0xD94

#define E_ACC3_PACKET_ERR0                  0xD05
#define E_ACC3_PACKET_ERR1                  0xD35
#define E_ACC3_PACKET_ERR2                  0xD65
#define E_ACC3_PACKET_ERR3                  0xD95

#define E_RBI_NOT_READY0                    0xD06
#define E_RBI_NOT_READY1                    0xD36
#define E_RBI_NOT_READY2                    0xD66
#define E_RBI_NOT_READY3                    0xD96

#define E_FLS_SCAN_OPERATIONAL0             0xD07   // スキャン機構動作中 
#define E_FLS_XCVR_OPERATIONAL0             0xD08   // 送受信ユニットが動作中 
#define E_FLS_COMM_ERROR0                   0xD09   // 通信異常 
#define E_FLS_OVERHEAT_ERROR0               0xD0A   // 温度異常 
#define E_FLS_VOLT_SUPPLY_ERROR0            0xD0B   // 電源電圧異常 
#define E_FLS_INTERNAL_ERROR0               0xD0C   // その他の異常 
#define E_FLS_RANGE_PERF_ERROR0             0xD0D   // 検知能力劣化 
#define E_FLS_UNKNOWN_ERROR0                0xD0E   // 定義されていないエラー
