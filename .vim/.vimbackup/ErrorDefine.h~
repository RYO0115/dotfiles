// ARS408 (Continental-Radar)
#define E_ARS408ODS_NOT_READY				0xC32	// E_ACC3_NOT_READYを継承
#define E_ARS408_NORESPONCE					0xD03	// E_ACC3_NORESPONCE0を継承
#define E_ARS408_PACKET_LOST				0xD05	// E_ACC3_PACKET_ERR0を継承
#define E_ARS408_CAN_ERROR					0xD09	// E_FLS_COMM_ERROR0を継承
#define E_ARS408_TEMPERATURE_ERROR			0xD0A	// E_FLS_OVERHEAT_ERROR0を継承
#define E_ARS408_VOLTAGE_ERROR				0xD0B	// E_FLS_VOLT_SUPPLY_ERROR0を継承
#define E_ARS408_INTERNAL_ERROR				0xD0C	// E_FLS_INTERNAL_ERROR0を継承
#define E_ARS408_TEMPORARY_ERROR			0xD0E	// E_FLS_UNKOWN_ERROR0を継承

