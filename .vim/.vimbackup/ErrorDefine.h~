/* Motor */
#define E_MOTOR_POT_ERROR                   0xE00
#define E_MOTOR_JUMP                        0xE02
#define E_MOTOR_LINECUT                     0xE03
#define E_MOTOR_SHORT                       0xE04

/* ODS Power */
#define E_RESET_ODS                         0xE05
#define E_RESET_LUX                         0xE06
#define E_RESET_MMR1C                       0xE06

/* LH Drive */
#define E_DRIVE_HEARTBEAT_CUT               0xE20
#define E_NO_NPOSITION                      0xE21
#define E_DRIVE_DEAD                        0xE22
#define E_TRUCK_TYPE_WRONG                  0xE24
#define E_EMGREQ_FROM_DRIVE                 0xE25
#define E_DRIVE_E_MODE                      0xE26
#define E_DRIVE_CAN_A                       0xE27
#define E_SWAPPED_LH                        0xE28
#define E_NO_DRIVE_CANA_HEARTBEAT           0xE29
